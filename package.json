{
  "name": "cssprefix",
  "version": "0.3.3",
  "title": "CSSPrefix",
  "description": "CSS Prefixer for JavaScript code. The small snipet that get or set vendor-prefixed CSS property or CSS value. This is not pre-compiler for CSS, this is used to handle those in JavaScript code.",
  "keywords": [
    "css",
    "vendor-prefix",
    "prefixer",
    "property",
    "value"
  ],
  "main": "css-prefix.min.js",
  "jsnext:main": "./src/css-prefix.js",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "cross-env": "^3.1.4",
    "webpack": "^2.2.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack --display-modules"
  },
  "homepage": "https://github.com/anseki/css-prefix",
  "repository": {
    "type": "git",
    "url": "git://github.com/anseki/css-prefix.git"
  },
